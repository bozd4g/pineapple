{
          "singleQuote": true,
          "trailingComma": "es5",
          "printWidth": 200,
          "overrides": [
                    {
                              "files": ".prettierrc",
                              "options": {
                                        "parser": "json"
                              }
                    }
          ],
          "editor.formatOnSave": true,
          "proseWrap": "always",
          "tabWidth": 4,
          "requireConfig": false,
          "useTabs": false,
          "bracketSpacing": true,
          "jsxBracketSameLine": false,
          "semi": true
}
